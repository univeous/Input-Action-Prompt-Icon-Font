func _ready() -> void:
	label_settings = template.get_child(0).label_settings
	for icon in icons:
		var new_item = template.duplicate()
		new_item.show()
		container.add_child(new_item)
		var icon_label:Label = new_item.get_child(0)
		var label:Label = new_item.get_child(1)
		icon_label.text = icon
		label.text = icon
		#var copy := func(e:InputEvent):
			#if e is InputEventMouseButton and e.is_pressed() and e.button_index == MOUSE_BUTTON_LEFT:
				#DisplayServer.clipboard_set(icon)
		#icon_label.gui_input.connect(copy)
		#label.gui_input.connect(copy)
	$VBox/HBoxContainer/SpinBox.value = label_settings.font_size
	$VBox/HBoxContainer/SpinBox2.value = label_settings.outline_size
	$VBox/HBoxContainer/ColorPickerButton.color = label_settings.font_color
	$VBox/HBoxContainer/ColorPickerButton2.color = label_settings.outline_color


func _on_spin_box_value_changed(value: float) -> void:
	label_settings.font_size = int(value)


func _on_spin_box_2_value_changed(value: float) -> void:
	label_settings.outline_size = int(value)


func _on_color_picker_button_color_changed(color: Color) -> void:
	label_settings.outline_color = color


func _on_icon_color_changed(color: Color) -> void:
	label_settings.font_color = color
